{
  "name": "jasloe/drupal-project",
  "description": "Project template for Drupal 9 projects. Forked from drupal-composer/drupal-project",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/admin_toolbar_tools": "^3.0",
    "drupal/adminimal_admin_toolbar": "^1.11",
    "drupal/adminimal_theme": "^1.6",
    "drupal/auto_entitylabel": "^3.0@beta",
    "drupal/components": "^2.2",
    "drupal/config_split": "^1.7",
    "drupal/core-composer-scaffold": "^9.0",
    "drupal/core-recommended": "^9.0",
    "drupal/field_group": "^3.1",
    "drupal/filefield_paths": "1.0-beta5",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_run": "^2.1@beta",
    "drupal/migrate_tools": "^5.0",
    "drupal/name": "^1.0@RC",
    "drupal/optional_end_date": "^1.1",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/token": "^1.9",
    "drupal/twig_tweak": "^3.0",
    "drupal/twig_xdebug": "^1.2",
    "drupal/views_bulk_operations": "^3.10",
    "drush/drush": "^10.3",
    "geerlingguy/drupal-vm": "dev-master",
    "vlucas/phpdotenv": "^5.1",
    "webflo/drupal-finder": "^1.2"
  },
  "require-dev": {
    "drupal/core-dev": "^9.0",
    "zaporylie/composer-drupal-optimizations": "^1.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "discard-changes": true,
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "allowed-packages": [
        "drupal/core"
      ],
      "file-mapping": {
        "[web-root]/.htaccess": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/htaccess",
          "overwrite": false
        },
        "[web-root]/sites/development.services.yml": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/development.services.yml",
          "overwrite": false
        },
        "[web-root]/sites/default/default.services.yml": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/default.services.yml",
          "overwrite": false
        },
        "[web-root]/sites/default/default.settings.php": {
          "mode": "replace",
          "path": "web/core/assets/scaffold/files/default.settings.php",
          "overwrite": false
        }
      },
      "composer-exit-on-patch-failure": true
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "patches": {}
  }
}
