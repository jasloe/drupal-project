.DS_Store
 **/.DS_Store

# Ignore developer local directories
.vagrant
/backups/
/config/

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore custom modules, themes
/web/modules/custom/*
/web/themes/custom/*

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env, et al. files as they are personal
*workspace
/.env
/.editorconfig
/.gitattributes
